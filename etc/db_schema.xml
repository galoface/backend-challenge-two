<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="infobase_customeraccess_customeraccess" resource="default" engine="innodb" comment="infobase_customeraccess_customeraccess Table">
		<column xsi:type="int" name="customeraccess_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="customeraccess_id"/>
		</constraint>
		<column name="customer_id" nullable="true" xsi:type="int" comment="customer_id" identity="false"/>
		<column name="customer_name" nullable="true" xsi:type="varchar" comment="customer_name" length="255"/>
		<column name="customer_email" nullable="true" xsi:type="varchar" comment="customer_email" length="255"/>
		<column name="access_time" nullable="true" xsi:type="datetime" comment="access_time"/>
		<column name="device" nullable="true" xsi:type="varchar" comment="device" length="255"/>
		<column name="url" nullable="true" xsi:type="varchar" comment="url" length="255"/>
		<column name="additional_data" nullable="true" xsi:type="text" comment="additional_data"/>
	</table>
	<table name="infobase_customeraccess_customeraccesspage" resource="default" engine="innodb" comment="infobase_customeraccess_customeraccesspage Table">
		<column xsi:type="int" name="customeraccesspage_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="customeraccesspage_id"/>
		</constraint>
		<column name="page" nullable="true" xsi:type="varchar" comment="page" length="255"/>
        <column name="page_route" nullable="true" xsi:type="varchar" comment="page" length="255"/>
	</table>
</schema>
